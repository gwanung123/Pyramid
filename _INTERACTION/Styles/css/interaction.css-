.keypad-area 
{
	position:relative;
	border:1px solid #bfbfbf;
	border-bottom:0;
	padding:10px 15px 12px;
	width: 300px;
}
.keypad-area .left {float:left;}
.keypad-area .record-icon{padding-right:8px;}
.iconbox {border:1px solid #e7e7e7;padding:3px;}
.keypad-area .stop-icon {float:right;margin-top:2px;}
.keypad-area .stop-icon:hover, .keypad-area .key-icon:hover {background:#dee0e2;}
.keypad-area .key-icon {position:absolute;bottom:20px;right:15px;}
.num-write {clear:both;margin:0;width:85%;font-size:24px;color:#808080;margin-top:21px;border:0;}
input.num-write::-webkit-input-placeholder {color:#bfbfbf;}



.call-func 
{
	position:relative;
	width: 300px;
}

.call-func li 
{
  float:left;
  background:#414141;
  width:25%;
  height:65px;
  line-height:65px;
  text-align:center;
}
.call-func li.numline:after 
{
  content:url("../img/line_wt.png");
  position: absolute;
  top:1%;
  left:49.8%;
}
.call-func li.numline_2:after 
{
  content:url("../img/line_wt.png");
  position: absolute;
  top:51%;
  left:49.8%;
}
.call-func li a 
{
  color:#bfbfbf;
  display:block;
  font-weight:600;
  height:65px;
}
.call-func li a:hover {background:#5a5a5a;}
.call-func li.nomg {margin-right:0;}
.call-func .icontxt {color:#fff;line-height:14px;font-size:11px;}
.call-func .icon 
{
	background:url("../img/interact/hold_30_btn.png") no-repeat 50% 50%;
	display:block;
	height:39px;
}
.call-func .retry {background:url("../img/retry_30_btn.png") no-repeat 50% 50%;}
.call-func .consult {background:url("../img/conference_30_btn.png") no-repeat 50% 50%;}
.call-func .transfer {background:url("../img/transfer_30_btn.png") no-repeat 50% 50%;}
.accept-icon 
