	.chat-box {
		display: inline-block;
		color: #34495e;
		margin: 0 0 30px 0;
		min-width: 90%;
	}
	
	.chat-box li {
		display: table;
		padding: 6px;
		margin: 6px 9px;
		font-size: 12px;
		line-height: 25px;
		-webkit-border-top-left-radius: 6px;
		-webkit-border-top-right-radius: 6px;
		-webkit-border-bottom-right-radius: 6px;
		-webkit-border-bottom-left-radius: 6px;
		-moz-border-radius-topleft: 6px;
		-moz-border-radius-topright: 6px;
		-moz-border-radius-bottomright: 6px;
		-moz-border-radius-bottomleft: 6px;
		border-top-left-radius: 6px;
		border-top-right-radius: 6px;
		border-bottom-right-radius: 6px;
		border-bottom-left-radius: 6px;
	}
	
	.chat-box .other,
	.chat-box .other-normal {
		font-size: 12px;
		background-color: #ecf0f1;
		max-width: 60%;
		clear: both;
		float: left;
	}
	
	.chat-box .other:before {
		content: ' ';
		position: relative;
		top: auto;
		bottom: auto;
		border: 12px solid;
		border-color: #ecf0f1 transparent transparent transparent;
		margin: 0 0 0 -18px;
		float: left;
	}
	
	.chat-box .me,
	.chat-box .me-normal {
		font-size: 12px;
		background-color: #C1E4EC;
		max-width: 60%;
		height: auto;
		clear: both;
		float: right;
	}
	
	.chat-box .me:before {
		content: ' ';
		position: relative;
		bottom: auto;
		border: 16px solid;
		border-color: #C1E4EC transparent transparent transparent;
		margin: 0px -18px 0 0;
		float: right;
	}
	
	.chat-box .me-id {
		font-size: 9px;
		margin: 5px 5px;
		float: right;
		padding: 0px;
		display: block;
		clear: both;
		background-color: transparent;
	}
	
	.chat-box .other-id {
		font-size: 9px;
		margin: -5px -5px;
		float: left;
		padding: 0px;
		display: block;
		clear: both;
		background-color: transparent;
	}
	
	.chat-box li span {
		margin: -20px -60px;
		font-size: 9px;
		color: #7f8c8d;
		display: block;
	}
	
	.chat-box .time {
		font-size: 9px;
		margin: 0px;
		padding: 0px;
		display: block;
		clear: both;
		background-color: transparent;

		/* font-size:12px; color:#adadad; float:right; margin-right:4%; */
	}
	
	.chat-box .id {
		font-size: 9px;
		margin: -10px -10px;
		/* float: right padding: 0px; */
		display: block;
		clear: both;
		background-color: transparent;
	}






