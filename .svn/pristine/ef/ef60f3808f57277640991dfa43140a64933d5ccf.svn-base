/***************************************
 * file : /Contents/containerDN.html
 * tree
 **************************************/

.nx-containerGridDN {
   height: 100%;
}

.nx-containerGridDN .k-tabstrip-wrapper {
   height: 100%;
}

.nx-containerGridDN-center {
   font-size: 12px;
   position: absolute;
   bottom: 0px;
   top: 29px;
   left: 0px;
   right: 0px;
   background-color: #fff;
   border: 0px;
   overflow: hidden;
}

/**
 * tabstrip-header
 */

.nx-containerGridDN-center > .k-tabstrip-items {
   font-weight: 600;
   margin: 0px !important;
}

.nx-containerGridDN-center > .k-tabstrip-items > .k-state-default {
   background-color: #c5c5c5 !important;
   background-image: none !important;
}

#nxContainer .nx-containerGridDN-center > .k-tabstrip-items > .k-state-hover {
   background-color: #17a2b8 !important;
   background-image: none !important;
}

#nxContainer .nx-containerGridDN-center > .k-tabstrip-items > .k-state-active {
   background-color: #17a2b8 !important;
   background-image: none !important;
}

#nxContainer .nx-containerGridDN-center > .k-tabstrip-items > .k-item {
   border: 0px !important;
   width: 50%;
   text-align: center !important;
}

#nxContainer .nx-containerGridDN-center > .k-tabstrip-items > .k-item > span.k-loading {
   border: 0px;
}

#nxContainer .nx-containerGridDN-center > .k-tabstrip-items > .k-item > span.k-link {
   color: #fff !important;
   padding-left: 5px;
}

#nxContainer .nx-containerGridDN-center > .k-tabstrip-items > .k-item > span.k-remove {
   padding: 0px;
   padding-left: 15px;
   height: 14px;
}

/**
 * tabstrip-body
 */

.nx-containerDN-center .k-content {
   margin: 0px !important;
   height: 91% !important;
   padding: 0px;
   border-style: dotted;
}

.nx-containerGridDN-center .k-content > .k-tabstrip-wrapper {
   height: 100%;
}

/**
 * grid
 */
/*
.k-grid-content {
   position: absolute;
   top: 62px;
   bottom: 0px;
}
*/

.nxContainerGridDN1,
.nxContainerGridDN2 {
    border-color: #fff;
    /* position: absolute; */
    position: relative;
    bottom: 0px;
    top: 0px;
    left: 0px;
    right: 0px;
    border: 0px;
    /* margin-top: 26px; */
    /* height: 90.5%; */
    height:100%;
}

.nxContainerGridDN1 .k-grid-toolbar,
.nxContainerGridDN2 .k-grid-toolbar {
   background-color: #fff;
   background-image: none;
   height: 26px;
}

.nxContainerGridDN1 .k-grid-header,
.nxContainerGridDN2 .k-grid-header {
   background-color: #5a5d63;
   background-image: none;
}

/* 20180430 Anna */
.nxContainerGridDN1 .k-header.k-grid-toolbar{
    display: none;
}

.nxContainerGridDN1 .k-grid-header th,
.nxContainerGridDN2 .k-grid-header th {
   background-color: #5a5d63;
   background-image: none;
   color: #fff;
}

.nxContainerGridDN1 .k-grid-content-locked,
.nxContainerGridDN2 .k-grid-content-locked {
   border-color: #aaa;
   border-right-width: 2px;
}

/* 20180430 Anna Modified */
.nxContainerGridDN1 .k-grid-content {
    position: absolute;
    top: 30px;
    bottom: 0px;
 }
 
 .nxContainerGridDN2 .k-grid-content {
    position: absolute;
    top: 64px;
    bottom: 0px;
 }
 /* end */

.nxContainerGridDN1 .k-pager-wrap,
.nxContainerGridDN2 .k-pager-wrap {
   background-color: #5a5d63;
   background-image: none;
   color: #fff;
   position: absolute;
   width: 100%;
   height: 25px;
   bottom: 0px;
   /* right: 0px;
   left: 0px; */
}

.nxContainerGridDN1 a.k-link:link,
.nxContainerGridDN2 a.k-link:link {
   color: #fff;
}

.nxContainerGridDN1 tbody .k-button,
.nxContainerGridDN2 tbody .k-button {
   min-width: 20px !important;
   padding-left: 15px;
   padding-right: 0px;
}

.nxContainerGridDN1 tbody .k-state-selected,
.nxContainerGridDN2 tbody .k-state-selected {
   background-color: #fff;
   color: #2e2e2e;
   font-weight: 600;
}

.nxContainerGridDN1 tr:hover,
.nxContainerGridDN2 tr:hover {
   background-color: #868e96;
}

.nxContainerGridDN1 td.k-state-selected:hover, .nxContainerGridDN1 tr.k-state-selected:hover,
.nxContainerGridDN2 td.k-state-selected:hover, .nxContainerGridDN2 tr.k-state-selected:hover {
   background-color: #868e96;
}

/* override absolute positioning styles of the validator messages */
.nxContainerGridDN1 .k-tooltip,
.nxContainerGridDN2 .k-tooltip {
   position: static;
   margin: 7px 0px !important;
   border: 0px;
   padding: 5px 5px;
}

/* hide validator tooltip callouts */
.nxContainerGridDN1 .k-tooltip .k-callout,
.nxContainerGridDN2 .k-tooltip .k-callout {
   display: none;
}

