'use strict';

define([],  function() {

   /**
    * 로그인 정보
    */
   function Signin( obj ) {
      var siginInfo = obj || {},
          userId = obj.userId || "",
          autoSignin = obj.autoSignin || false,
          mainHosts = obj.mainHosts || [],
          pushHosts = obj.pushHosts || [],
          authHosts = obj.authHosts || [],
          logHosts = obj.logHosts || [];

      this.getSiginInfo = function() {
         return siginInfo;
      };

      this.setUserId = function( arg0 ) {
         userId = arg0;
      };

      this.getUserId = function() {
         return userId;
      };

      this.setAutoSignin = function( arg0 ) {
         autoSignin = arg0;
      };

      this.getAutoSignin = function() {
         return autoSignin;
      };

      this.setMainHosts = function( arg0 ) {
         mainHosts = arg0;
      };

      this.getMainHosts = function() {
         return mainHosts;
      };

      this.setPushHosts = function( arg0 ) {
         pushHosts = arg0;
      };

      this.getPushHosts = function() {
         return pushHosts;
      };

      this.setAuthHosts = function( arg0 ) {
         authHosts = arg0;
      };

      this.getAuthHosts = function() {
         return authHosts;
      };

      this.setLogHosts = function( arg0 ) {
         logHosts = arg0;
      };

      this.getLogHosts = function() {
         return logHosts;
      };
   };

   return Signin;

});
