@font-face{font-family:roboto; src:url('../font/Roboto-Regular.ttf');}
*{margin:0; padding:0;}
li{list-style-type:none;}
img{border:0 none;}
body{font-family:'roboto', Arial, sans-serif; color:#000; font-size:100%; background-color:#e3e7ea; }
a:link, a:visited{color:#000; text-decoration:none;}
a:hover, a:focus{color:#000; text-decoration:none;}

.hidden{position:absolute; left:-9999px; width:1px; height:1px; overflow:hidden; font-size:0; line-height:0;}
.container{margin:0 !important;}

/* wrapper layout */
   #wrapper{width:100%; height:100%; overflow:hidden;}

/* header layout */
   #header{width:100%; height:55px; background-color:#fff; overflow:hidden;}
   #header .container{width:100%; height:55px; line-height:55px;}

/* header left */
   #header .h_left .h_logo{margin:0 !important; padding-left:25px; font-weight:bold; float:left; color:#414141;}
   #header .h_left .h_logo a{font-size:20px; color:#6d6e71;}
   /* #header .container .h_left .h_logo img{width:115px; margin-left:20px; margin-top:21px;} */

/* header logon_info */
   #header .logon_info{width:300px; height:55px; float:right; background-color:#ffffff; overflow:hidden;}
   #header .logon_info .logon_state1{width:130px; height:55px; line-height:20px; font-size:13px; color:#808080; float:left;}
   
   /* #header .logon_info .logon_state1 .logon_dn{width:105px; height:20px; float:left; margin-top:7px;}
   #header .logon_info .logon_state1 .logon_name{width:40px; height:20px; float:left; color:#494949; font-weight:bold; margin-top:7px;}
   #header .logon_info .logon_state1 .logon_time{width:150px;} */

   #header .logon_info .logon_state1 .logon_dn{width:85px; line-height:55px; float:left; padding-left:5px;}
   #header .logon_info .logon_state1 .logon_name{width:40px; line-height:55px; float:left; color:#494949; font-weight:bold;}
   #header .logon_info button{width:70px; height:26px; color:#5e92d1; font-size:12px; border-radius:5px; border:2px solid #5e92d1; background-color:#ffffff; font-weight:600;}
   #header .logon_info button:hover{background-color:#5e92d1; color:#ffffff; cursor:pointer;}

/* main layout */
   #main{width:100%; height:100%; padding-top:25px; overflow:hidden; background-color:#e3e7ea;}

/* main logon_ctn */
   #main .logon_ctn{width:100%; height:100%; margin-top:5px;}
   #main .logon_ctn .container{}

   #main .logon_ctn .type_top, #main .logon_ctn .type_bottom{width:80%; text-align:center; margin-left:10%;}
   #main .logon_ctn ul li a:link, #main .logon_ctn ul li a:visited{width:calc(25% - 20px); height:250px; float:left; padding-top:45px; color:#9a9a9a; background-color:#fff; margin-right:20px; margin-bottom:20px; display:block;}
   #main .logon_ctn ul li a:hover, #main .logon_ctn ul li a:focus{background-color:#5e92d1; color:#ffffff;}
   #main .logon_ctn ul li span{width:78%; margin-left:11%; display:block; font-size:13px; font-weight:400; line-height:19px;}
   #main .logon_ctn ul li img{margin-bottom:30px;}
   #main .logon_ctn ul li .type_title{margin-bottom:35px; font-size:15px; font-weight:600;}

