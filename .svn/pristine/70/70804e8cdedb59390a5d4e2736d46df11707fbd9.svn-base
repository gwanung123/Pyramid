.title p{background:#f9fafa; color:#9a9a9a; text-align:center; font-size:14px; line-height:30px; font-weight:600; border-bottom:1px solid #e2e2e2; margin-bottom:0px;} */

.queue{height:110px; padding:15px; border-bottom:1px solid #e2e2e2; text-align:center;}
.queue p{font-size:13px; line-height:25px; color:#414141; font-weight:bold; margin-bottom:0px;}
.queue span{color:#6986d8; font-size:40px; line-height:45px;}

.detail_cnt{height:330px; text-align:center;}
.detail_cnt li{width:50%; height:110px; padding:20px 0px; float:left;}
.detail_cnt li p{margin-bottom:5px; font-size:13px; line-height:25px; color:#808080; font-weight:600;}
.detail_cnt li span{font-size:30px; line-height:35px; font-weight:100; color:#494949;}

.waiting, .missed{border-bottom:1px solid #e2e2e2;}
.waiting, .abandon{border-right:1px solid #e2e2e2;}

/* 0410 add */
#nxUserAgentListGrid,
#nxUserAgentINGrid,
#nxUserAgentOUTGrid,
#nxUserQueueListGrid {
    overflow:hidden;
}
.k-pager-wrap.k-grid-pager.k-widget.k-floatwrap {
    position: absolute;
    bottom: 0px;
    left: 0px;
    width: 100%;
    background-image: none;
    background-color: #eae8e8;
    padding-left: calc(50% - 70px);
}
.k-pager-wrap.k-grid-pager.k-widget.k-floatwrap a:link{
    color:#808080;
}
.k-pager-wrap.k-grid-pager.k-widget.k-floatwrap a:hover{
    background-color: transparent;
    background-image: none;
}
th.k-header {
    background-image: none;
}
.k-pager-numbers .k-reset .k-link:hover {
    background-color: transparent;
    background-image: none;
}

.detail_cnt li:nth-child(3), 
.detail_cnt li:nth-child(4) {
    border-top:1px solid #ccc;
    border-bottom:1px solid #ccc;
}

.detail_cnt li:nth-child(1), 
.detail_cnt li:nth-child(3),
.detail_cnt li:nth-child(5) {
    border-right:1px solid #ccc;
}

.nx-user-box.a6 .detail_cnt li{
    height:165px;
    padding:45px 0;
}

.nx-user-box.a6 .detail_cnt li:nth-child(3) span{
    color:#42cb70;
}

.nx-user-queue-bottom td:nth-child(1) h5.nx-user-queue-bottom-value{
    color:#3bc4f0;
}
.nx-user-queue-bottom td:nth-child(2) h5.nx-user-queue-bottom-value{
    color:#006ade;
}
.nx-user-queue-bottom td:nth-child(3) h5.nx-user-queue-bottom-value{
    color:#d4d4d4;
}

.nx-user-agentState-bottom td:nth-child(1) h5.nx-user-agentState-bottom-value{
    color:#ff6800;
}
.nx-user-agentState-bottom td:nth-child(2) h5.nx-user-agentState-bottom-value{
    color:#a0a700;
}
.nx-user-agentState-bottom td:nth-child(3) h5.nx-user-agentState-bottom-value{
    color:#ff8d00;
}
.nx-user-agentState-bottom td:nth-child(4) h5.nx-user-agentState-bottom-value{
    color:#678900;
}
