/***************************************
 * file : /Contents/user.html
 **************************************/
#nx-contents-user {
   width:100%;
   height:100%;
   padding:20px;
}

#nxUserTopButtons {
   font-weight: 600;
   font-size: 14px;
   /* padding: 10px 30px; */
}

#nxUserTopButtons li {
    /* width: 150px;
    display: block;
    padding: 0;
    height: 32px; */
    width: 135px;
    display: block;
    padding: 0;
    height: 32px;
    border-radius: 0px;
    border-color: #e6e6e6;
    margin-right: 8px;
}

.nx-contents-user-table {
   width: 100%;
}

.nx-contents-user-top-row {
    height: 30px;
    margin-bottom: 15px;
    width: 100%;
    float: left;
}

.k-button a{
    color: #494949;
    font-size: 12px;
    background-color: #fff;
    display: block;
    height: 30px;
    line-height: 30px;
    text-decoration: none;
}

.k-button a:hover{
    background-color:#808080;
    color:#fff;
}

.nx-user-box-content {
    border: 1px solid #ccc;
}

.title,
.nxcss-user-title {
    text-align: center;
    height: 30px;
    font-weight: 600;
    line-height: 29px;
    background-color: #fafafa;
    margin-bottom: 0px;
    font-size: 14px;
    border-bottom: 1px solid #ccc;
    color: #808080;
}

h5.nx-user-agentState-bottom-value,
h5.nx-user-queue-bottom-value {
    color:#494949;
    font-size: 20px;
    font-weight: bold;
}

h6.nx-user-agentState-bottom-title,
h6.nx-user-queue-bottom-title {
    font-size: 13px;
    color: #808080;
    margin-bottom: 5px;
}

tr.nx-user-agentState-bottom,
tr.nx-user-queue-bottom {
    height: 63px;
}

#nx-monitor-user-queue-call {
   height: 360px;
}

#nxUserQueueChart,
#nxUserAgentStateChart {
    width: 250px;
    height: 250px;
    margin: 0 auto 15px;
}

.nx-user-agentState-top,
.nx-user-queue-top {
   border-bottom: 1px solid #ccc;
}

.nx-user-agentState-bottom-col,
.nx-user-queue-bottom-col {
   width: 69px;
   text-align: center;
   padding-top: 10px;
}

table.nx-user-agentState-table,
table.nx-user-queue-table {
    width: 100%;
}

