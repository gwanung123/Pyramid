<script>

    $(function(){
        $(".analy button a").addClass("selected");
        $(".analy button a").click(function(){
            $($(this).attr("href")).toggle(function(){});
            $(this).toggleClass("selected");

            if($(".selected").length<=5){
                $(".agent_boxes").animate({height:"170px"});
                //sliding
            }
            else{
                $(".agent_boxes").animate({height:"325px"});
            }

            return false;
        });
        
